{
  "name": "webimar-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@turf/turf": "^7.2.0",
    "@types/geojson": "^7946.0.16",
    "@types/jest": "^27.5.2",
    "@types/leaflet": "^1.9.18",
    "@types/leaflet-draw": "^1.0.12",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.9.0",
    "geojson": "^0.5.0",
    "leaflet": "^1.9.4",
    "leaflet-draw": "^1.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.18",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(axios|@turf|geojson|react-leaflet|leaflet|polyclip-ts)/)"
    ],
    "moduleNameMapper": {
      "^axios$": "axios/dist/node/axios.cjs",
      "\\.(css|less|scss)$": "identity-obj-proxy",
      "^react-leaflet$": "<rootDir>/src/__mocks__/react-leaflet.js",
      "^leaflet$": "<rootDir>/src/__mocks__/leaflet.js",
      "^@turf/turf$": "<rootDir>/src/__mocks__/turf.js",
      "^../services/api$": "<rootDir>/src/__mocks__/api.ts",
      "^../../services/api$": "<rootDir>/src/__mocks__/api.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts",
      "!src/index.tsx",
      "!src/reportWebVitals.ts"
    ]
  },
  "devDependencies": {
    "identity-obj-proxy": "^3.0.0"
  }
}
